@using DotNetCoreWeb.DTO
@using MyProject.Core
@model List<GoodsOutput>
<h1>商品列表</h1>

<table class="table">
    <thead>
        <tr>
            <td>序号</td>
            <td>商品名称</td>
            <td>操作</td>
        </tr>
    </thead>
    @foreach (GoodsOutput item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.Name</td>
            <td>@Html.Raw(item.Operate)</td>
        </tr>
    }
</table>
